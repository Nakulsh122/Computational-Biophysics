# Makefile to clean up generated files and run the Python script

# Run the Python script
run:
	python3 Assignment_1.py

# Remove all output files
clean:
	rm -f *.log *.asa *.pdb *.fasta *.rsa

# Remove only log files
clean_logs:
	rm -f *.log

# Remove only ASA files
clean_asa:
	rm -f *.asa

# Remove only PDB files
clean_pdb:
	rm -f *.pdb

# Remove only FASTA files
clean_fasta:
	rm -f *.fasta

# Remove only RSA files
clean_rsa:
	rm -f *.rsa
